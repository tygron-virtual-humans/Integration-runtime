%
% This is a test of a domestic robot by Tristan,
% and it is part of the EIS distribution.
% It was working with GOAL 3133, tested #1763
%

environment{
	env = "Domestic-1.1.1-SNAPSHOT.jar".
	
	init = [gui = "on"].
}

agentfiles {
	"owner.goal".
	"robot.goal".
	"supermarket.goal".
}

launchpolicy {
	when [name=robot]@env do launch robot:robot.
    when [name=owner]@env do launch owner:owner.
	when [name=supermarket]@env do launch supermarket:supermarket.
}
