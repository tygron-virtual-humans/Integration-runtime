environment{
    env = "Tic-Tac-Toe-1.1.1-SNAPSHOT.jar".
    init = [xplayer = "agent",  oplayer = "agent"].
}

agentfiles{
    % insert (list of) agent file references below.
    "tictactoe.goal" [name = player].
}

launchpolicy{
    % insert appropriate launch rules for your environment and agents below.
    when [name=xplayer]@env do launch x : player.
	when [name=oplayer]@env do launch o : player.
}
